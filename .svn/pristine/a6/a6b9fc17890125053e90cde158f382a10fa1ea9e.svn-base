package kr.or.ddit.medical.clinc.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("Inspection")
public class InspectionController {
	
	// 검사실 대기환자 목록 조회
	@GetMapping("InspecPatientList.do")
	public String InspecPatientList() {
		return "medi/inspection";
	}
	
	// 검사실 대기환자 상세 조회
	@GetMapping("InspecPatientDetail.do")
	public String InspecPatientDetail() {
		return "medi/inspection";
	}
	
	// 검사실 환자 과거 진료 내역 조회
	@GetMapping("InspecPatientPast.do")
	public String InspecPatientPast() {
		return "medi/inspection";
	}
	
	// 검사실 환자 결과 출력
	@GetMapping("InspectionResult.do")
	public String InspectionResult() {
		return "medi/inspection";
	}
	
	
	
}
