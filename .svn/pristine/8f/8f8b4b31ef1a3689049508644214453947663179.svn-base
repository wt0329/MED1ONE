.wrapper{
  position: relative;
    .timepicker-cf{
      height:60px;
      width:100%;
      position: relative;
    }
    .timepicker{
      position:absolute;
      top:200px;
      width:97%;
      height:0px;
      background:var(--primary-color);
      overflow:hidden;
        .owl, .owl-stage{
          width:100%;
          height:60px;
          display:flex;
          align-items: center;
          .owl-item{
            font-size:1.1em;
            display:inline-block;
            font-weight:600;
            color:white;
            text-align:center;
            min-width:100px;
            opacity:0.4;
            cursor:pointer;
            transition:all .2s ease-in-out;
          }
          .center-n{
            font-size:1.5em;
            opacity:0.7;
          }
          .center{
            font-size:2em;
            opacity:1;
          }
        }
    }
    .fade-l{
    position:absolute;
    height:100%;
    width:150px;
    left:0;
    top:0;
    background: -moz-linear-gradient(left, rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%);
    background: -webkit-linear-gradient(left, rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    background: linear-gradient(to right, rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=1 );
    }
    .fade-r{
    position:absolute;
    height:100%;
    width:150px;
    right:0;
    top:0;
    background: -moz-linear-gradient(left, rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%);
    background: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%);
    background: linear-gradient(to right, rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#ffffff',GradientType=1 );
    }
    .owl-prev, .owl-next{
    z-index:20;
    position:absolute;
    width:20px;
    top:20px;
    color:#667483;
    cursor:pointer;
    transition:.2s ease-in-out;
    background:none;
    border:0;
    font-size:1em;
    }
    .owl-prev{
    left:20px;
    
    &:before{
        content: '\f053';
        font-family: FontAwesome;
    }
    &:hover{
        left:18px;
        transition:.2s ease-in-out;
    }
    }
    .owl-next{
    right:20px;
    
    &:hover{
        right:18px;
        transition:.2s ease-in-out;
    }
    &:before{
        content: '\f054';
        font-family: FontAwesome;
    }
    }
    .inner-wrap{
      .request{
        display: flex;
        align-items: center;
        justify-content: space-between;
        width:100%;
        background:var(--primary-color);
        color:#fff;
        font-weight:600;
        cursor:pointer;
        opacity:1;
        br{
          display:none;
        }
        .sendRequest{
          position:absolute;
          right:20px;
          top:16px;
          
          &:before{
            content: '\f054';
            font-family: FontAwesome;
          }
        }
        &.disabled{
          opacity:0.8;
          pointer-events:none;
          transition:all .3s ease-in-out;
        }
      }
      .disabled span{
        display:none;
      }
    }
    #calendar{
      overflow:hidden;
      position:relative;
      .monthChange{
        background:#fff;
        width:100%;
        height:0;
        position:absolute;
        top:160px;
        z-index:20;
      }
  
      .ui-datepicker{
        display: none;
        width: 100%;
        margin:auto;
        padding:30px 0px;
        text-transform: uppercase;
        background: var(--card-color);
        transition:all .2s ease-in-out;
        .ui-datepicker-header {
          position: relative;
          margin-bottom: 10px;
  
          .ui-datepicker-title { 
            text-align: center;
            font-weight:700;
            color:#353C42;
  
            .ui-datepicker-month {
              position: relative;
              padding-right:5px;
            }
          }
          .ui-datepicker-prev,
          .ui-datepicker-next {
            top:-5px;
            position: absolute;
            padding: 5px;
            cursor: pointer;
          }
          .ui-datepicker-prev {
            left:0;
          }
          .ui-datepicker-next {
            right:0;
          }
          .ui-datepicker-prev:before{
            content: "\f053";
            font-family: FontAwesome;
          }
          .ui-datepicker-next:after{
            content: "\f054";
            font-family: FontAwesome;
          }
          .ui-datepicker-prev span,
          .ui-datepicker-next span{
            display: inline-block;
            text-indent: -9999px;
          }
        }
        .ui-datepicker-calendar{
          width:100%;
          th{
            line-height: 30px;
            padding-top: 15px;
            text-align: center;
            font-weight: 600;
            font-size:0.95em;
            color: var(--font-color);
          }
          td{
            padding: 0 7px;
            text-align: center;
            height:34px;
            font-weight:600;
            cursor:pointer;
            box-sizing:border-box;
            transition:all .2s ease-in-out;
            
            &:hover{
              background:var(--secondary-color);
              transition:all .2s ease-in-out;
            }
          }
          .ui-datepicker-current-day{
            background:var(--secondary-color);
            border-bottom:2px solid rgb(53, 60, 66);
            &.open{
              margin-bottom:100px;
            }
          }
          .ui-datepicker-today{
            .ui-state-default{
              border-bottom:2px solid rgba(53, 60, 66, 0.5);
            }
          }
          .ui-state-default{
            text-decoration:none;
            color: var(--font-color);
          }
          .ui-datepicker-other-month{
            pointer-events:none;
            .ui-state-default{
              color: #919BA3;
            }
          }
        }
      }
    }
}

  @media only screen and (max-width: 600px)  {
    .wrapper .inner-wrap .request br{
      display:block;
    }
  }