.cal-img{
	@media only screen and (max-width: 600px)  {
		height: 200px;
	}
}
.calandarclock-block{
    position: relative;   
	@media only screen and (max-width: 600px)  {
		display: none; 
	}
	
.outer {
    position: absolute;
    left: 30%;
    bottom: 0px;
	z-index: 1;
	-webkit-transform: translateX(-40%);
	-moz-transform: translateX(-40%);
	-ms-transform: translateX(-40%);
	-o-transform: translateX(-40%);
	transform: translateX(-40%);
	cursor: pointer;
    ul{
        list-style: none;
        padding-left: 0px;
    }
	&:hover {
		.inner {
			-webkit-transform: rotate(0) translate(0);
			-moz-transform: rotate(0) translate(0);
			-ms-transform: rotate(0) translate(0);
			-o-transform: rotate(0) translate(0);
			transform: rotate(0) translate(0);
		}
	}
	&:active {
        list-style: none;
		.inner {
			-webkit-transform: rotate(0) translate(0) scale(0.9);
			-moz-transform: rotate(0) translate(0) scale(0.9);
			-ms-transform: rotate(0) translate(0) scale(0.9);
			-o-transform: rotate(0) translate(0) scale(0.9);
			transform: rotate(0) translate(0) scale(0.9);
		}
		.front {
			.date {
				-webkit-transform: scale(2);
			}
			.day {
				visibility: hidden;
				opacity: 0;
				-webkit-transform: scale(0);
				-moz-transform: scale(0);
				-ms-transform: scale(0);
				-o-transform: scale(0);
				transform: scale(0);
			}
			.month {
				visibility: hidden;
				opacity: 0;
				-webkit-transform: scale(0);
				-moz-transform: scale(0);
				-ms-transform: scale(0);
				-o-transform: scale(0);
				transform: scale(0);
			}
		}
		.right {
			-webkit-transform: rotate(-5deg) translateX(80px) scale(0.9);
			-moz-transform: rotate(-5deg) translateX(80px) scale(0.9);
			-ms-transform: rotate(-5deg) translateX(80px) scale(0.9);
			-o-transform: rotate(-5deg) translateX(80px) scale(0.9);
			transform: rotate(-5deg) translateX(80px) scale(0.9);
		}
		.left {
			-webkit-transform: rotate(5deg) translateX(-80px) scale(0.9);
			-moz-transform: rotate(5deg) translateX(-80px) scale(0.9);
			-ms-transform: rotate(5deg) translateX(-80px) scale(0.9);
			-o-transform: rotate(5deg) translateX(-80px) scale(0.9);
			transform: rotate(5deg) translateX(-80px) scale(0.9);
		}
		.calendarMain {
			-webkit-transform: scale(1.8);
			opacity: 0;
			visibility: hidden;
		}
		.clock {
			-webkit-transform: scale(1.4);
			opacity: 1;
			visibility: visible;
		}
		.calendarNormal {
			bottom: -30px;
			opacity: 1;
			visibility: visible;
		}
		.year {
			top: -30px;
			opacity: 1;
			visibility: visible;
			letter-spacing: 3px;
		}
	}
}

.signboard {
	width: 100px;
	height: 100px;
	margin: auto;
	color: #fff;
	border-radius: 10px;
    ul{
        list-style: none;
        padding-left: 0px;
    }
}
.front {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 3;
	background: #ff726b;
	text-align: center;
}
.right {
	position: absolute;
	right: 0;
	z-index: 2;
    -webkit-transform: rotate(5deg) translate(-4px, 4px);
	-moz-transform: rotate(5deg) translate(-4px, 4px);
	-ms-transform: rotate(5deg) translate(-4px, 4px);
	-o-transform: rotate(5deg) translate(-4px, 4px);
	transform: rotate(5deg) translate(-4px, 4px);
	background: #3498DB;
}
.left {
	position: absolute;
	left: 0;
	z-index: 1;
	-webkit-transform: rotate(5deg) translate(-4px, 4px);
	-moz-transform: rotate(5deg) translate(-4px, 4px);
	-ms-transform: rotate(5deg) translate(-4px, 4px);
	-o-transform: rotate(5deg) translate(-4px, 4px);
	transform: rotate(5deg) translate(-4px, 4px);
	background: #3498DB;
}
.calendarMain {
	width: 100%;
	height: 100%;
	position: absolute;
	opacity: 1;
}
.month {
	font-size: 10px;
	line-height: 30px;
	font-weight: 600;
	text-transform: uppercase;
	letter-spacing: 3px;
}
.day {
	font-size: 10px;
	line-height: 30px;
	font-weight: 600;
	text-transform: uppercase;
	letter-spacing: 3px;
}
.date {
	font-size: 40px;
	line-height: 40px;
	font-weight: 300;
	text-transform: uppercase;
	letter-spacing: 3px;
}
.clock {
	width: 100%;
	height: 100%;
	position: absolute;
	font-size: 40px;
	line-height: 100px;
	font-weight: 300;
	text-transform: uppercase;
	letter-spacing: 3px;
	text-align: center;
	opacity: 0;
	visibility: hidden;
}
.year {
	width: 100%;
	position: absolute;
	top: 0;
	font-size: 14px;
	line-height: 30px;
	font-weight: 300;
	text-transform: uppercase;
	letter-spacing: 0;
	text-align: center;
	opacity: 0;
	visibility: hidden;
	color: #ff726b;
}
.calendarNormal {
	width: 100%;
	position: absolute;
	bottom: 0;
	font-size: 14px;
	line-height: 30px;
	font-weight: 600;
	text-transform: uppercase;
	letter-spacing: 3px;
	text-align: center;
	opacity: 0;
	visibility: hidden;
}
.date2 {
	color: #ff726b;
}
.day2 {
	color: #3498DB;
}
.month2 {
	color: #EFC94C;
}
.dribbble {
	position: absolute;
	top: -60px;
	font-size: 14px;
	opacity: 0;
}
em {
	color: #ed4988;
}
.designer {
	width: 100%;
	height: 50%;
	position: absolute;
	bottom: 0;
	text-align: center;
	opacity: 0;
	li {
		width: 100%;
		position: absolute;
		bottom: 30%;
	}
	a {
		width: 30px;
		height: 30px;
		display: block;
		position: relative;
		border-radius: 100%;
		margin: auto;
		color: rgba(46, 204, 113, 0.55);
		&:after {
			content: "see designs";
			position: absolute;
			top: 0;
			left: 40px;
			font-size: 14px;
			line-height: 33px;
			font-weight: 600;
			text-transform: uppercase;
			letter-spacing: 2px;
			white-space: nowrap;
		}
		&:hover {
			&:after {
				color: #2ecc71;
			}
		}
	}
	img {
		display: block;
		border-radius: 100%;
	}
}
}